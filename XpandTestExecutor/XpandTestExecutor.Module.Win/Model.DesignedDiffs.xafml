<?xml version="1.0" encoding="utf-8"?>
<Application>
  <Views>
    <DetailView Id="EasyTest_DetailView">
      <Items>
        <PropertyEditor Id="FileName" OpenPathWithExplorer="True" />
      </Items>
    </DetailView>
    <DetailView Id="EasyTestExecutionInfo_DetailView">
      <Items>
        <PropertyEditor Id="TestsLog" PropertyEditorType="Xpand.ExpressApp.Win.PropertyEditors.RichEdit.RichEditWinPropertyEditor">
          <RichEdit HighLightExtension="xml" ControlBindingProperty="ControlText" ShowToolBars="False" />
        </PropertyEditor>
        <PropertyEditor Id="WebLog" PropertyEditorType="DevExpress.ExpressApp.HtmlPropertyEditor.Win.HtmlPropertyEditor" />
        <PropertyEditor Id="WinLog" PropertyEditorType="DevExpress.ExpressApp.HtmlPropertyEditor.Win.HtmlPropertyEditor" />
      </Items>
    </DetailView>
    <ListView Id="ExecutionInfo_ListView">
      <Columns>
        <ColumnInfo Id="Duration">
          <DurationSettings DisplayTotal="Minutes" />
        </ColumnInfo>
      </Columns>
    </ListView>
  </Views>
</Application>